                    <item name="filter" xsi:type="string">text</item>
                    <item name="label" xsi:type="string" translate="true">shipping type</item>
                </item>
            </argument>
        </column>
        <column name="residential" class="Kitchen\Ordertsk\Ui\Component\Listing\Column\Mycolumn">
