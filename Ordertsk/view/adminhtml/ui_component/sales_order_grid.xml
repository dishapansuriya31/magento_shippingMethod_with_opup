        <column name="delivery_appointment" class="Kitchen\Ordertsk\Ui\Component\Listing\Column\Mycolumn">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="label" xsi:type="string" translate="true">delivery appointment</item>
                </item>
